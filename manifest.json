{
	"name": "Hello World!",
	"description": "My first Chrome App.",
	"version": "0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": [
				"background.js"
			]
		}
	},
	"icons": {
		"16": "calculator-16.png",
		"128": "calculator-128.png"
	},
	"commands": {
		"toggle-feature-foo": {
			"suggested_key": {
				"default": "Ctrl+Shift+Y"
			},
			"description": "Toggle feature foo",
			"global": true
		}
	}
}
